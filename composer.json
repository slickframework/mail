{
  "name": "slick/mail",
  "description": "Email plugin provides generalized functionality to compose and send both text and MIME-compliant multipart email messages.",
  "minimum-stability": "dev",
  "license": "MIT",
  "type": "library",
  "version": "v1.1.0",
  "keywords": [
    "mail", "e-mail", "MIME", "HTML e-mail message", "send e-mail"
  ],
  "authors": [
    {
      "name": "Filipe Silva",
      "email": "silvam.filipe@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {

  },
  "require-dev": {
    "phpunit/phpunit": "^4",
    "phpunit/php-code-coverage": "^2",
    "scrutinizer/ocular": "~1.1",
    "behat/behat": "^3.0.4"
  },
  "autoload": {
    "psr-4": {
      "Slick\\Mail\\": "src"
    }
  },
  "autoload-dev": {
